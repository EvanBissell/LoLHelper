
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:visibility="visible"
    tools:context="com.fourfoureight.lolhelper.JungleTimer" >

    <ImageButton
        android:id="@+id/imageButton1"
        style="@style/ColorRed"
        android:background="#ffffff"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:adjustViewBounds="true"
        android:onClick="RedGolem"
        android:contentDescription="@id/imageButton1"
        android:scaleType="centerInside"
        android:src="@drawable/golem" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:background="#ffffff"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_alignTop="@+id/imageButton1"
        android:adjustViewBounds="false"
        android:onClick="RedLizard"
        android:contentDescription="@id/imageButton2"
        android:scaleType="fitCenter"
        android:src="@drawable/lizard" />

    <ImageButton
        android:id="@+id/ImageButton3"
        android:background="#ffffff"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:adjustViewBounds="true"
        android:onClick="BlueGolem"
        android:contentDescription="@id/ImageButton3"
        android:scaleType="centerInside"
        android:src="@drawable/golem" />

    <ImageButton
        android:id="@+id/ImageButton4"
        android:background="#ffffff"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/ImageButton3"
        android:adjustViewBounds="false"
        android:onClick="BlueLizard"
        android:contentDescription="@id/ImageButton4"
        android:scaleType="fitCenter"
        android:src="@drawable/lizard" />

    <ImageButton
        android:id="@+id/ImageButton5"
        android:background="#ffffff"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:adjustViewBounds="true"
        android:onClick="Dragon"
        android:contentDescription="@id/ImageButton5"
        android:scaleType="centerInside"
        android:src="@drawable/dragon" />

    <ImageButton
        android:id="@+id/ImageButton6"
        android:background="#ffffff"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:adjustViewBounds="false"
        android:onClick="Baron"
        android:contentDescription="@id/ImageButton6"
        android:scaleType="fitCenter"
        android:src="@drawable/baron" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageButton1"
        android:layout_alignRight="@+id/imageButton1"
        android:text="GR"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageButton2"
        android:layout_alignRight="@+id/imageButton2"
        android:text="LR"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ImageButton3"
        android:layout_alignRight="@+id/ImageButton3"
        android:text="GB"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ImageButton4"
        android:layout_alignRight="@+id/ImageButton4"
        android:text="LB"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ImageButton5"
        android:layout_alignRight="@+id/ImageButton5"
        android:text="DR"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/ImageButton6"
        android:layout_alignRight="@+id/ImageButton6"
        android:text="BA"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ImageButton3"
        android:layout_alignTop="@+id/ImageButton3"
        android:text="Golem Blue"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ImageButton4"
        android:layout_alignTop="@+id/ImageButton4"
        android:text="Lizard Blue"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButton1"
        android:layout_alignTop="@+id/imageButton1"
        android:text="Golem Red"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButton2"
        android:layout_alignTop="@+id/imageButton2"
        android:text="Lizard Red"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ImageButton5"
        android:layout_alignTop="@+id/ImageButton5"
        android:text="Dragon"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ImageButton6"
        android:layout_alignTop="@+id/ImageButton6"
        android:text="Baron"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</RelativeLayout>